{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "MISTRAL_API_KEY": "@mistral_api_key"
  },
  "functions": {
    "app.py": {
      "maxDuration": 60
    }
  }
} 